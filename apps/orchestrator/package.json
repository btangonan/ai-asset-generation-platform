{
  "name": "@ai-platform/orchestrator",
  "version": "1.0.0",
  "description": "AI asset generation orchestrator service",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -e .env.local -- tsx watch src/index.ts",
    "start": "dotenv -e .env.local -- node dist/index.js",
    "worker": "dotenv -e .env.local -- tsx watch src/worker.ts",
    "worker:prod": "dotenv -e .env.local -- node dist/worker.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ai-platform/clients": "workspace:*",
    "@ai-platform/shared": "workspace:*",
    "@ai-platform/sheets": "workspace:*",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^9.0.0",
    "@google-cloud/pubsub": "^4.11.0",
    "@google-cloud/storage": "^7.0.0",
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.2",
    "fastify": "^4.24.0",
    "googleapis": "^126.0.0",
    "p-limit": "^7.1.1",
    "pino": "^8.16.0",
    "pino-pretty": "^10.0.0",
    "sharp": "^0.34.3",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "dotenv-cli": "^10.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}