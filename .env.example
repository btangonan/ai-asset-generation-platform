# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# Google Cloud Storage
GCS_BUCKET=your-project-ai-renders

# Pub/Sub Configuration
PUBSUB_TOPIC_IMAGES=image-processing
PUBSUB_SUBSCRIPTION_IMAGES=image-processing-sub

# Google Sheets Configuration  
GOOGLE_SHEETS_API_KEY=your-sheets-api-key
GOOGLE_SHEETS_ID=your-sheet-id

# Gemini API Configuration
GEMINI_API_KEY=your-gemini-api-key

# Application Configuration
NODE_ENV=development
PORT=9090
LOG_LEVEL=info

# Rate Limiting
MAX_ROWS_PER_BATCH=10
MAX_VARIANTS_PER_ROW=3
USER_COOLDOWN_MINUTES=10

# Cost Configuration
RUN_MODE=dry_run
DAILY_BUDGET_USD=100

# Security
SIGNED_URL_EXPIRY_DAYS=7

# API Authentication (Generate secure keys for production)
# Format: aip_ + base64url(32 random bytes)
# Generate with: node -e "console.log('aip_' + Buffer.from(Array.from({length:32}, () => Math.floor(Math.random()*256))).toString('base64url'))"
AI_PLATFORM_API_KEY_1=your-secure-api-key-1
AI_PLATFORM_API_KEY_2=your-secure-api-key-2
AI_PLATFORM_API_KEY_3=your-secure-api-key-3

# Cloud Run Configuration (for deployment)
CLOUD_RUN_SERVICE=ai-orchestrator
CLOUD_RUN_REGION=us-central1
CLOUD_RUN_MIN_INSTANCES=1
CLOUD_RUN_MAX_INSTANCES=100